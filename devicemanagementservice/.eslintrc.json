{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "func-names": ["error", "never"],
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0,
        "linebreak-style": 0
    }
}