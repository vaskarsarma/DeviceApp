{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {    
        "no-console": "off",
        "no-eval": "error",
        "import/first": "error"    
    }
}